{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "schematics-example": {
      "description": "A blank schematic.",
      "factory": "./schematics-example/index#schematicsExample"
    },
    "simple-file-create": {
      "description": "A simple ts file creation schematic with given console log and class name.",
      "factory": "./simple-file-create/index#simpleFileCreate",
      "schema": "./simple-file-create/schema.json"
    },
    "schematic-with-template": {
      "description": "A schematic that use to create templates.",
      "factory": "./schematic-with-template/index#schematicWithTemplate",
      "schema": "./schematic-with-template/schema.json"
    }
  }
}
